@article{gaujena_analysis_2020,
  title = {Analysis of {{Thermal Parameters}} of {{Hemp Fiber Insulation}}},
  author = {Gaujena, Baiba and Agapovs, Vladislavs and Borodinecs, Anatolijs and Strelets, Ksenia},
  date = {2020-12-03},
  journaltitle = {Energies},
