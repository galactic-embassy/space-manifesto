@article{ercan_jenny_robotic_2021,
  title = {Robotic {{Plaster Spraying}}: {{Crafting Surfaces}} with {{Adaptive Thin-Layer Printing}}},
  shorttitle = {Robotic {{Plaster Spraying}}},
  author = {Ercan Jenny, Selen and Lloret-Fritschi, Ena and Jenny, David and Sounigo, Eliott and Tsai, Ping-Hsun and Gramazio, Fabio and Kohler, Matthias},
  date = {2021-08-27},
  journaltitle = {3D Printing and Additive Manufacturing},
  publisher = {{Mary Ann Liebert, Inc., publishers}},
  issn = {2329-7662},
  doi = {10.1089/3dp.2020.0355},
  url = {https://www.liebertpub.com/doi/full/10.1089/3dp.2020.0355},
  urldate = {2021-09-13},
  abstract = {Embedded in a long tradition of craftsmanship, inside or outside building surfaces, is often treated with plaster, which plays both functional and ornamental roles. Today, plasterwork is predominantly produced through rationalized, time-, and cost-efficient processes, used for standardized building elements. These processes have also gained interest in the construction robotics field, and while such approaches target the direct automation of standardized plasterwork, they estrange themselves from the inherent qualities of this malleable material that are well known from the past. This research investigates the design potentials of robotic plaster spraying, proposing an adaptive, thin-layer vertical printing method for plasterwork that aims to introduce a digital craft through additive manufacturing. The presented work is an explorative study of a digitally controlled process that can be applied to broaden the design possibilities for the surfaces of building structures. It involves the spraying of multiple thin layers of plaster onto a vertical surface to create volumetric formations or patterns, without the use of any formwork or support structures. This article describes the experimental setup and the initial results of the data collection method involving systematic studies with physical testing, allowing to develop means to predict and visualize the complex-to-simulate material behavior, which might eventually enable to design with the plasticity of this material in a digital design tool.},
  keywords = {adaptive fabrication,data-driven prediction models,robotic plaster spraying,shot-earth,Shotcrete 3D printing,thin-layer printing,urgent_1,visualization of material behavior},
  file = {/Users/jelle/Dropbox/Academic/Papers/storage/E8C6QP9Y/Ercan Jenny et al_2021_Robotic Plaster Spraying.pdf}
}

@article{ercan_jenny_robotic_2022,
