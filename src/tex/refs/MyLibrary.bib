  author = {Burger, Joris and Lloret-Fritschi, Ena and Scotto, Fabio and Demoulin, Thibault and Gebhard, Lukas and Mata-Falcón, Jaime and Gramazio, Fabio and Kohler, Matthias and Flatt, Robert J.},
  date = {2020-04-01},
  journaltitle = {3D Printing and Additive Manufacturing},
