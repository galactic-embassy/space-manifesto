@article{bhandarkar_migrating_2000,
  title = {Migrating from {{IGES}} to {{STEP}}: One to One Translation of {{IGES}} Drawing to {{STEP}} Drafting Data},
  shorttitle = {Migrating from {{IGES}} to {{STEP}}},
  author = {Bhandarkar, Mangesh P and Downie, Blair and Hardwick, Martin and Nagi, Rakesh},
  date = {2000-05-01},
  journaltitle = {Computers in Industry},
  shortjournal = {Computers in Industry},
  volume = {41},
