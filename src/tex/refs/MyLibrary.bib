  shorttitle = {Robotic {{Equilibrium}}},
  booktitle = {{{ACADIA}} // 2018: {{Recalibration}}. {{On}} Imprecisionand Infidelity. [{{Proceedings}} of the 38th {{Annual Conference}} of the {{Association}} for {{Computer Aided Design}} in {{Architecture}} ({{ACADIA}}) {{ISBN}} 978-0-692-17729-7] {{Mexico City}}, {{Mexico}} 18-20 {{October}}, 2018, Pp.  342-349},
