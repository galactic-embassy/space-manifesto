@article{marcos_multi-objective_2012,
  title = {A Multi-Objective Approach for the Motion Planning of Redundant Manipulators},
  author = {Marcos, Maria da Graça and Tenreiro Machado, J.A. and Azevedo-Perdicoúlis, T.-P.},
  date = {2012-02},
  journaltitle = {Applied Soft Computing},
