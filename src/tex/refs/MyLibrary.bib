@inproceedings{van_de_voorde_sculpture_2006,
  title = {Sculpture House in {{Belgium}} by {{Jacques Gillet}}},
  booktitle = {Design and {{Nature III}}: {{Comparing Design}} in {{Nature}} with {{Science}} and {{Engineering}}},
  author = {Van de Voorde, S. and De Meyer, R. and De Kooning, E. and Taerwe, L. and Van De Walle, R.},
  date = {2006-05-18},
  volume = {1},
